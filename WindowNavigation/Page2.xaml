<Page x:Class="WindowNavigation.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WindowNavigation"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page2">
    <StackPanel>
        <TextBlock Text="2nd Page" />
        <TextBlock>
            <Hyperlink Click="GoBack">Page 1</Hyperlink>
        </TextBlock>
        <TextBlock>
            <Hyperlink Click="GoForward">Page 3</Hyperlink>
        </TextBlock>
        <TextBlock x:Name="Greetings"/>
    </StackPanel>
</Page>
